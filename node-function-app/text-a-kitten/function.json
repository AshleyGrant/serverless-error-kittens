{
  "disabled": false,
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "table",
      "direction": "in",
      "name": "kittens",
      "connection": "ERROR_KITTENS_STORAGE",
      "tableName": "%IMAGE_INFO_TABLE_NAME%",
      "filter": "isACat eq true and isSFW eq true"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "signalR",
      "name": "message",
      "hubName": "errorKittens",
      "direction": "out"
    }
  ],
  "scriptFile": "../dist/text-a-kitten/index.js"
}
