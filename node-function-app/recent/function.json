{
  "disabled": true,
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "type": "table",
      "direction": "in",
      "name": "kittens",
      "connection": "ERROR_KITTENS_STORAGE",
      "tableName": "%IMAGE_INFO_TABLE_NAME%",
      "filter": "isACat eq true and isSFW eq true"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "scriptFile": "../dist/recent/index.js"
}
